<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <section name="specFlow" type="TechTalk.SpecFlow.Configuration.ConfigurationSectionHandler, TechTalk.SpecFlow" />
  </configSections>
  <specFlow>
    <plugins>
      <add name="SpecRun" />
    </plugins>
    <unitTestProvider name="MsTest" />
  
  </specFlow>
  <system.net>
    <defaultProxy useDefaultCredentials="true">
    </defaultProxy>
  </system.net>
  <appSettings>
    <add key="Environment" value="Auto" />
    <add key="Browser" value="FireFox" />       
    <add key="TestURL" value="https://weather-acceptance.herokuapp.com/" />       
  </appSettings>
</configuration>